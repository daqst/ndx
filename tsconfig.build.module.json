{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "outDir": "dist/module",
    "sourceMap": true,
    "inlineSourceMap": false,
    "importHelpers": true,
    "noEmitHelpers": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "**/__tests__"
  ]
}